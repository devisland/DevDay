<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Evento DevDay - Belo Horizonte - Outubro/2012" />
    <meta name="author" content="DevIsland" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="stylesheet" media="all" href="@Url.Content("~/Content/css/bootstrap.css")" />
    <link rel="stylesheet" media="all" href="@Url.Content("~/Content/css/font-awesome.css")" />
    <link rel="stylesheet" media="all" href="@Url.Content("~/Content/css/portfolio.css")" />
    <link href="http://fonts.googleapis.com/css?family=Oxygen" rel="stylesheet" type="text/css">
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
</head>
<body>
    <div class="container">
        <div class="">
            <br />
            <img src="@Url.Content("~/Content/img/devday2.png")" width="200px" alt="" />
            <ul class="nav nav-pills pull-right">
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>@Html.ActionLink("Palestras", "Index", "Palestras")</li>
                <li>@Html.ActionLink("Inscrições", "Index", "Inscricoes")</li>
                <li>@Html.ActionLink("Local", "Index", "Local")</li>
                <li>@Html.ActionLink("Agenda", "Index", "Agenda")</li>
                <li>@Html.ActionLink("Patrocinadores", "Index", "Patrocinadores")</li>
                <li>@Html.ActionLink("Edições Anteriores", "Index", "Gallery")</li>
                <li>@Html.ActionLink("Contato", "Index", "Contato")</li>
                <li>@Html.ActionLink("Divulgue", "Index", "Divulgue")</li>
            </ul>
        </div>
        @RenderBody()
    </div>
    <br />
    <div class="container">
        <div class="row-fluid show-grid">
            <div class="span6">
                <h3 style="color: gray">
                    <strong>Patrocinadores Ouro</strong></h3>
                <ul class="thumbnails">
                    <li class="span3"><a href="http://www.una.br" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/ouro/logouna.jpg")"  alt="UNA" />
                    </a></li>
                    <li class="span3"><a href="http://arkhi.com.br/" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/ouro/logoarkhi.jpg")"  alt="Arkhi" />
                    </a></li>
                    <li class="span3"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                </ul>
            </div>
            <div class="span6">
                <h3 style="color: gray">
                    <strong>Patrocinadores Prata</strong></h3>
                <ul class="thumbnails">
                    <li class="span3"><a href="http://www.frameworksystem.com/" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/prata/logoframework.jpg")" alt="Framework System" />
                    </a></li>
                    <li class="span3"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                    <li class="span3"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                    <li class="span3"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                </ul>
            </div>
        </div>
        <div class="row-fluid show-grid">
            <div class="span6">
                <h4 style="color: gray">
                    <strong>Patrocinadores Bronze</strong></h4>
                <ul class="thumbnails">
                    <li class="span2"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                    <li class="span2"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                    <li class="span2"><a href="@Url.Content("~/Content/download/DevDay2012.pdf")" class="thumbnail" target="_blank" >
                        <img src="http://placehold.it/216x148&text=Sua%20Marca" alt="" />
                    </a></li>
                </ul>
            </div>
            <div class="span6">
                <h4 style="color: gray">
                    <strong>Apoio</strong></h4>
                <ul class="thumbnails">
                    <li class="span2"><a href="http://twitter.com/horaextrabh" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/apoio/logohoraextrabh.png")" width="70" alt="horaextrabh" />
                    </a></li>
                    <li class="span2"><a href="http://twitter.com/pangeanet" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/apoio/logopangea.jpg")" width="70" alt="pangeanet" />
                    </a></li>
                    <li class="span2"><a href="http://python-mg.github.com/" class="thumbnail" target="_blank">
                        <img src="@Url.Content("~/Content/img/patrocinadores/apoio/logopythonmg.png")" width="70" alt="pythonmg" />
                    </a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="footer">
        <div class="container">
            <div class="fb-like" data-href="https://www.facebook.com/DevIsland" data-send="false" data-width="650"
                data-show-faces="true" data-action="recommend">
            </div>
            <div>
                &copy; DevIsland 2012
            </div>
        </div>
    </div>
    <script src="http://code.jquery.com/jquery.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.ui.map.js")" type="text/javascript"></script>
    @RenderSection("js", false)
    <script type="text/javascript">
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/pt_BR/all.js#xfbml=1&appId=255164787916716";
            fjs.parentNode.insertBefore(js, fjs);
        } (document, 'script', 'facebook-jssdk'));</script>
</body>
</html>