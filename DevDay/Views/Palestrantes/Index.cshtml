@section js
{
    <script src="@Url.Content("~/Content/js/bootstrap-tab.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $('#myTab a').click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

        var hashes = window.location.href.slice(window.location.href.indexOf('#') + 1).split('&');
        if (hashes.length === 1) {
            $('#myTab a[href="#' + hashes[0] + '"]').tab('show');
        }
    </script>
}

